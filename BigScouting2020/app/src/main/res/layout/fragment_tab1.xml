<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.Tab1">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:context=".ui.main.Tab1">

        <CheckBox
            android:id="@+id/autoCrossing"
            style="@style/Widget.AppCompat.CompoundButton.CheckBox"
            android:layout_width="232dp"
            android:layout_height="53dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="116dp"
            android:checked="false"
            android:text="Auto Crossing?"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:layout_width="60dp"
            android:layout_height="51dp"
            android:layout_marginTop="111dp"
            android:layout_marginEnd="25dp"
            android:text="-"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonPlus2"
            android:layout_width="60dp"
            android:layout_height="51dp"
            android:layout_marginStart="76dp"
            android:layout_marginTop="12dp"
            android:text="+"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintStart_toEndOf="@+id/lowGoalAuto"
            app:layout_constraintTop_toBottomOf="@+id/buttonPlus1" />

        <Button
            android:id="@+id/buttonPlus"
            android:layout_width="60dp"
            android:layout_height="51dp"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="508dp"
            android:text="-"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <Button
            android:id="@+id/button10"
            android:layout_width="60dp"
            android:layout_height="51dp"
            android:layout_marginStart="252dp"
            android:layout_marginBottom="500dp"
            android:text="-"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/buttonPlus2" />

        <TextView
            android:id="@+id/lowGoalAuto"
            android:layout_width="163dp"
            android:layout_height="43dp"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="509dp"
            android:text="Auto Low Goal:"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="38dp"
            android:layout_height="46dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="515dp"
            android:ems="10"
            android:inputType="number"
            android:text="0"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/buttonPlus2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/lowGoalAuto"
            app:layout_constraintTop_toBottomOf="@+id/editText2"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/buttonPlus1"
            android:layout_width="60dp"
            android:layout_height="51dp"
            android:layout_marginTop="110dp"
            android:text="+"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="255dp" />

        <EditText
            android:id="@+id/editText2"
            android:layout_width="40dp"
            android:layout_height="43dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="35dp"
            android:ems="10"
            android:inputType="number"
            android:text="0"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintBottom_toTopOf="@+id/editText3"
            app:layout_constraintEnd_toStartOf="@+id/buttonPlus1"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/autoCrossing" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="163dp"
            android:layout_height="43dp"
            android:layout_marginStart="16dp"
            android:text="Auto High Goal:"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="109dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <CheckBox
        android:id="@+id/checkBox2"
        style="@style/Widget.AppCompat.CompoundButton.CheckBox"
        android:layout_width="232dp"
        android:layout_height="53dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="116dp"
        android:checked="false"
        android:text="Auto Crossing?"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>